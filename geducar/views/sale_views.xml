<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="geducar_view_order_form" model="ir.ui.view">
            <field name="name">geducar.sale.order.form</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <field name="date_order" position="after">
                    <field name="cantidad_alumnos"/>
                </field>
            </field>
        </record>

        <record id="geducar_view_quotation_tree" model="ir.ui.view">
            <field name="name">geducar.sale.order.tree</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_quotation_tree"/>
            <field name="arch" type="xml">
                <field name="partner_id" position="after">
                    <field name="cantidad_alumnos"/>
                </field>
            </field>
        </record>

</odoo>
